{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [],
      "env": [],
      "outputs": []
    },
    "type-check": {
      "dependsOn": ["^type-check"]
    },
    "clean": {
      "dependsOn": ["^clean"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "test": {},
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "pre-build": {
      "dependsOn": ["^pre-build"]
    }
  }
}
